#!/bin/bash

if [ -f "/etc/hamonikr/info" ] ; then

    DEFAULT_TESTPAGE_SRC_FILE=/etc/hamonikr/templates/default-testpage.pdf
    DEFAULT_TESTPAGE_TARGET_FILE=/usr/share/cups/data/default-testpage.pdf

    if [[ -e $DEFAULT_TESTPAGE_SRC_FILE ]] && [[ -e $DEFAULT_TESTPAGE_TARGET_FILE ]]; then
        cp $DEFAULT_TESTPAGE_SRC_FILE $DEFAULT_TESTPAGE_TARGET_FILE
    fi

fi